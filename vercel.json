{
  "version": 2,
  "builds": [
    {
      "src": "api/download.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/download",
      "dest": "/api/download.py"
    }
  ],
  "functions": {
    "api/download.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
